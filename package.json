{
  "name": "my-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": ["plaintext", "markdown", "tex", "html", "global", "all"],
        "path": "./snippets/all.json"
      },
      {
        "language": ["json"],
        "path": "./snippets/json.json"
      },
      {
        "language": [
          "javascript",
          "javascriptreact",
          "javascript.jsx",
          "typescript",
          "typescriptreact",
          "typescript.jsx"
        ],
        "path": "./snippets/javascript.json"
      },
      {
        "language": [
          "javascriptreact",
          "javascript.jsx",
          "typescriptreact",
          "typescript.jsx"
        ],
        "path": "./packer/start/friendly-snippets/snippets/html.json"
      },
      {
        "language": [
          "javascriptreact",
          "javascript.jsx",
          "typescriptreact",
          "typescript.jsx"
        ],
        "path": "./snippets/javascript-solidjs.json"
      },
      {
        "language": ["gitcommit"],
        "path": "./snippets/gitcommit.json"
      },
      {
        "language": [
          "html",
          "javascriptreact",
          "javascript.jsx",
          "typescriptreact",
          "typescript.jsx"
        ],
        "path": "./snippets/html.json"
      },
      {
        "language": [
          "css"
        ],
        "path": "./snippets/css.json"
      },
      {
        "language": ["make"],
        "path": "./snippets/make.json"
      },
      {
        "language": ["http"],
        "path": "./snippets/http.json"
      },
      {
        "language": ["just"],
        "path": "./snippets/just.json"
      },
      {
        "language": ["serif"],
        "path": "./snippets/serif.json"
      },
      {
        "language": ["dockerfile"],
        "path": "./snippets/dockerfile.json"
      },
      {
        "language": ["yaml"],
        "path": "./snippets/yaml.json"
      },
      {
        "language": ["markdown"],
        "path": "./snippets/slidesdown.json"
      },
      {
        "language": ["desktop"],
        "path": "./snippets/desktop.json"
      }
    ]
  }
}
