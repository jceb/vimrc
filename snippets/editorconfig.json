{
  "Template": {
    "prefix": "template",
    "body": [
      "# https://EditorConfig.org",
      "# https://github.com/editorconfig/editorconfig/wiki/EditorConfig-Properties",
      "",
      "# top-most EditorConfig file",
      "root = true",
      "",
      "[*]",
      "charset = utf-8",
      "end_of_line = lf",
      "indent_size = 2",
      "indent_style = space",
      "insert_final_newline = true",
      "trim_trailing_whitespace = false",
      "max_line_length = 120"
    ]
  }
}
