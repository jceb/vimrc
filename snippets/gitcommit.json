{
  "Insert TG- prefix": {
    "prefix": "tg",
    "body": "TG-${1}"
  },

  "Conventional commits message": {
    "prefix": "m",
    "body": "${1|feat,fix,chore,docs,style,ci,refactor,perf,test|}${2}: ${3:commit message}"
  },

  "Conventional commits message with ticket": {
    "prefix": "t",
    "body": "${1|feat,fix,chore,docs,style,ci,refactor,perf,test|}(${2:scope}): ${3:commit message} TG-${4:123}"
  }
}
