{
  "HTML/XML tag": {
    "prefix": ["tag"],
    "body": ["<${1:tag}${2}>${3}</${1}>"]
  },
  "Multi-line HTML/XML tag": {
    "prefix": "tag",
    "body": ["<${1:tag}${2}>", "${3}", "</${1}>"]
  },
  "Key Value Pair": {
    "prefix": "kv",
    "body": ["${1:key}=\"${2:value}\""]
  },
  "class": {
    "prefix": "class",
    "body": ["class=\"${1}\""]
  },
  "href": {
    "prefix": "href",
    "body": ["href=\"${1}\""]
  },
  "style": {
    "prefix": "style",
    "body": ["style=\"${1}\""]
  }
}
